@model IEnumerable<UserSignupLogin.Models.TBLUserInfo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Signup")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdUs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsernameUs)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PasswordUs)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.RePasswordUs)
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdUs)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsernameUs)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PasswordUs)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.RePasswordUs)
        </td>*@
        <td>
            @Html.ActionLink("Edit", "Edit", new { IdUs = item.IdUs }) |
            @Html.ActionLink("Details", "Detail", new { IdUs = item.IdUs }) |
            @Html.ActionLink("Delete", "Delete", new { IdUs = item.IdUs  })
        </td>
    </tr>
}

</table>
