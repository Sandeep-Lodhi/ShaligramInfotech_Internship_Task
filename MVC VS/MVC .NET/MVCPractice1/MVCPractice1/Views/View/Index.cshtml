
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h1>ViewBag</h1>
        @ViewBag.myProp
        @ViewBag.myProp2

        <ul>
            @foreach (var item in ViewBag.myList)
            {
                <li>@item</li>
            }
        </ul>

        <ul>
            @foreach (var item in ViewBag.emp)
            {
                <li>Name - @item.Name Id - @item.Id Email - @item.email</li>
            }
        </ul>

        <h1>ViewData</h1>
        @ViewData["key"]

    </div>
</body>
</html>
